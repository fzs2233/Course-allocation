{
  "name": "course-allocation",
  "version": "1.0.0",
  "scripts": {
    "dev:server": "node server.js",
    "dev:frontend": "cd frontend && npm run serve",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "setup": "npm install && npm run install:frontend",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\""
  },
  "devDependencies": {
    "babel-jest": "^29.7.0",
    "concurrently": "^8.0.1",
    "jest": "^29.7.0",
    "truffle-contract-size": "^2.0.1"
  },
  "dependencies": {
    "chai": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ethers": "^5.8.0",
    "express": "^4.21.2",
    "inquirer": "^8.2.6",
    "keypress": "^0.2.1",
    "mocha": "^11.1.0",
    "sinon": "^20.0.0",
    "testdouble": "^3.20.2",
    "web3": "^1.10.4"
  }
}
